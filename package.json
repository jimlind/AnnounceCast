{
    "name": "discord.podcasts",
    "description": "Discord Podcasts Bot",
    "version": "0.11.0",
    "private": false,
    "license": "AGPL-3.0",
    "author": "Jim Lind",
    "repository": {
        "type": "git",
        "url": "https://github.com/jimlind/discord.podcasts"
    },
    "engines": {
        "node": ">=16"
    },
    "type": "module",
    "main": "dist/app.js",
    "scripts": {
        "clean": "rm -rf dist",
        "build": "npm run clean && tsc",
