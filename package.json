        "dev": "npm run build && node dist/app.js dev"
    },
    "dependencies": {
        "@discordjs/opus": "^0.5.0",
        "@types/better-sqlite3": "^5.4.1",
        "awilix": "^4.3.4",
        "axios": "^0.21.1",
        "better-sqlite3": "^7.4.1",
        "boolean": "^3.0.3",
        "discord.js": "^12.5.3",
        "dotenv": "^8.2.0",
        "htmlparser2": "^6.1.0",
        "log4js": "^6.3.0",
        "normalize-url": "^6.0.0",
        "pretty-ms": "^7.0.1",
        "signal-exit": "^3.0.3",
        "turndown": "^7.0.0"
    },
    "devDependencies": {
        "@types/glob": "^7.1.3",
        "@types/node": "^15.0.2",
        "@types/signal-exit": "^3.0.0",
        "@types/sqlite3": "^3.1.7",
        "@types/turndown": "^5.0.0",
        "@types/ws": "^7.4.4",
        "typescript": "^4.2.4"
    }
